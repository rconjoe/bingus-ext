{
  "name": "bingus!",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "i'm just glad to be here man. let me know what you need.",
  "permissions": ["activeTab"],
  "action": {
    "default_icon": {
      "16": "assets/icons/16.png",
      "32": "assets/icons/32.png",
      "48": "assets/icons/48.png",
      "128": "assets/icons/128.png"
    },
    "default_title": "bingus!",
    "default_popup": "index.html#/action",
    "theme_icons": [
      {
        "light": "assets/icons/16.png",
        "dark": "assets/icons/16-dark.png",
        "size": 16
      },
      {
        "light": "assets/icons/32.png",
        "dark": "assets/icons/32-dark.png",
        "size": 32
      },
      {
        "light": "assets/icons/48.png",
        "dark": "assets/icons/48-dark.png",
        "size": 48
      },
      {
        "light": "assets/icons/128.png",
        "dark": "assets/icons/128-dark.png",
        "size": 128
      }
    ]
  }
}
